{
  "name": "ui-wiz",
  "description": "UIWiz Toolkit",
  "version": "0.1.0-beta.5",
  "sideEffects": false,
  "type": "module",
  "author": "Milad Afkhami <dev.miladafkhami@gmail.com> (https://milad-afkhami.com)",
  "displayName": "UIWiz Toolkit",
  "private": false,
  "homepage": "https://ui-wiz.milad-afkhami.com",
  "keywords": [
    "react",
    "design system",
    "UI kit",
    "styled-components"
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run lint && rm -rf build && rollup -c",
    "format": "prettier --write .",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "minor": "npm version minor -m \"build: minor package version to %s\"",
    "patch": "npm version patch -m \"build: patch package version to %s\"",
    "prepare": "husky prepare",
    "release:minor": "yarn minor && git push && npm publish",
    "release:patch": "yarn patch && git push && npm publish",
    "semantic-release": "semantic-release",
    "storybook:build": "yarn stylesheets && storybook build",
    "storybook:dev": "yarn stylesheets && storybook dev -p 6006",
    "storybook:start": "http-server storybook-static -p 6006",
    "stylesheets": "style-wiz constantsDir=src/constants/stylesheets destinationDir=public/stylesheets/style-wiz",
    "test": "jest",
    "typedoc": "typedoc src/index.ts --name \"UIWiz\" --readme README.md --plugin typedoc-plugin-markdown --excludeExternals && git add docs/* && git commit -m \"docs: autogenerate typedocs\""
  },
  "types": "build/types/index.d.ts",
  "main": "./build/index.js",
  "module": "./build/index.cjs.js",
  "typings": "./build/types/index.d.ts",
  "exports": {
    "require": "./build/index.cjs.js",
    "import": "./build/index.js",
    "types": "./build/types/index.d.ts"
  },
  "files": [
    "build/**/*"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/milad-afkhami/ui-wiz.git"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "~18.2.0",
    "@commitlint/config-conventional": "~18.1.0",
    "@rollup/plugin-node-resolve": "~15.2.3",
    "@rollup/plugin-typescript": "~11.1.4",
    "@storybook/addon-essentials": "7.5.0",
    "@storybook/addon-interactions": "7.5.0",
    "@storybook/addon-links": "7.5.0",
    "@storybook/addon-mdx-gfm": "7.5.0",
    "@storybook/addon-onboarding": "1.0.8",
    "@storybook/blocks": "7.5.0",
    "@storybook/preview-api": "~7.5.3",
    "@storybook/react": "7.5.0",
    "@storybook/react-webpack5": "7.5.0",
    "@storybook/testing-library": "0.2.2",
    "@testing-library/dom": "~10.1.0",
    "@testing-library/jest-dom": "~6.4.5",
    "@testing-library/react": "~15.0.6",
    "@testing-library/user-event": "~14.4.3",
    "@types/jest": "~29.5.5",
    "@types/lodash-es": "~4.17.9",
    "@types/node": "latest",
    "@types/react": "latest",
    "@types/react-dom": "latest",
    "@typescript-eslint/eslint-plugin": "~6.9.1",
    "@typescript-eslint/parser": "~6.9.1",
    "eslint": "~8.57.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-airbnb-typescript": "~17.0.0",
    "eslint-config-prettier": "~8.5.0",
    "eslint-plugin-import": "~2.29.0",
    "eslint-plugin-jsx-a11y": "~6.8.0",
    "eslint-plugin-react": "~7.33.2",
    "eslint-plugin-react-hooks": "~4.6.0",
    "eslint-plugin-security": "~1.7.1",
    "eslint-plugin-storybook": "~0.6.15",
    "goober": "~2.1.13",
    "husky": "~9.0.11",
    "jest": "~29.6.1",
    "jest-environment-jsdom": "~29.6.1",
    "prettier": "~2.5.1",
    "pretty-quick": "~3.1.3",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "rollup": "~3.29.3",
    "serve": "~14.2.1",
    "storybook": "7.5.0",
    "style-wiz": "~0.3.9",
    "ts-jest": "29.1.1",
    "ts-wiz": "~0.2.0",
    "tslib": "~2.6.2",
    "typedoc": "~0.25.3",
    "typedoc-plugin-markdown": "~3.16.0",
    "typescript": "~5.3.0"
  },
  "peerDependencies": {
    "goober": "~2.1.x",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "style-wiz": "~0.3.9"
  }
}
